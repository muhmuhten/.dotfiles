se ai noar bs=2 nocp dy=lastline enc=utf-8 hi=10000 hls is ls=2 sta wmnu

se sw=0 ts=4 lbr nu nuw=7 mouse= cm=blowfish2
se ls=2 stl=%F\ %y%r%h%w%m\ =%02B\ (%l,%v)[%L/%02p%%]
se swf dir=~/.vimstore bk bdir=~/.vimstore bkc=yes,breakhardlink
au BufWritePre * let &bex = strftime('@%F-%T~')

no U <C-r>
se t_kB=[Z
" reclaim <S-Tab> from supertab
au VimEnter * no! <S-Tab> <Tab>

sy on
filet plugin indent on

" wall of shame: not updated for shiftwidth(), but I use them anyway
au FileType css,java,perl,tex let &sw = &ts

au BufWritePost ~/public_html/*.html sil! !gzip -kf <afile>

exe pathogen#infect('{local,bundle}/{}')
let g:SuperTabDefaultCompletionType = 'context'

ru macros/matchit.vim
